name: dockercompose9805689979932424797
services:
  blog-backend:
    build:
      context: F:\Development\ASP.NET_Blog\Blog-Backend
      dockerfile: Blog-Backend\Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: blogbackend
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
