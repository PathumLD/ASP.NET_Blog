          create_host_path: true
      - type: bind
        source: C:\Users\Roshelle\AppData\Roaming/ASP.NET/Https
        target: C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https
        read_only: true
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose9805689979932424797_default